<%- include("_header") -%>
<div>
    <h2>Find Customers</h2>
    <h3>Total number of records in the database: </h3>
</div>
  
<form action="" method="post">
  <div class="form-group row col-6">
    <label for="ID5" class="col col-form-label ">ID</label>
    <div class="col-sm-10">
      <input type="number" class="form-control" id="ID5">(equal)
    </div>
  </div>
  <div class="form-group row col-6">
    <label for="fn5" class="col-sm-2 col-form-label">First Name</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="fn5">(begins with)
    </div>
  </div>
  <div class="form-group row col-6">
      <label for="ln5" class="col-sm-2 col-form-label">Last Name</label>
      <div class="col-sm-10">
        <input type="text" class="form-control" id="ln5">(begins with)
      </div>
    </div>
    <div class="form-group row col-6">
      <label for="st5" class="col-sm-2 col-form-label">State</label>
      <div class="col-sm-10">
        <input type="text" class="form-control" id="st5" maxlength="2" size="2" pattern="[AZ]{2}">(equal)
      </div>
    </div>
    <div class="form-group row col-6">
      <label for="YT5" class="col-sm-2 col-form-label">Sales YTD</label>
      <div class="col-sm-10">
        <input type="number" class="form-control" id="YT5">(greater than or equal to)
      </div>
    </div>
    <div class="form-group row col-6">
      <label for="py5" class="col-sm-2 col-form-label">Previous Year Sales</label>
      <div class="col-sm-10">
        <input type="number" class="form-control" id="py5">(greater than or equal to)
      </div>
    </div>
  <div class="form-group ">
      <div class="row">
    <div class="col-4">
      <button type="submit" class="btn btn-primary">Find</button>
    </div>
    <div class="col-4">
      <button type="reset" class="btn btn-primary">Reset</button>
    </div>
    </div>
  </div>
</form>
<%- include("_footer") -%>